<?xml version="1.0" encoding="UTF-8"?>
<type name="ViewWrapper" extends="Writable" xmlns="urn:quest-com:forge-types-1-0">
	<property name="name" type="String"/>
	<property name="id" type="String"/>
	<property name="view" type="Object"/>
	<property name="childViews" type="List&lt;ViewWrapper&gt;">
		<set property="access">READ_ONLY</set>
		<getter><![CDATA[return wrapChildViews(view);]]></getter>
	</property>
	<property name="viewType" type="forge.definitions.View"/>
</type>
